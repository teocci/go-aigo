    <meta name="viewport" content="width=device-width">
    <title>{{.Title}}</title>
    <link rel="stylesheet" href="/css/style.css">

    {{ template "styles" . }}
    <script type="module" src="/js/main.js" defer></script>
    <script src="/js/constant.js" defer></script>
